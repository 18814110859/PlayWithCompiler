/**
演示类的继承和多态。
*/

class Mammal{
    void speak(){
        println("mammal speeking...");
    }
}

class Cow extends Mammal{
    void speak(){
        println("moo~~ moo~~");
    }
}

class Sheep extends Mammal{
    void speak(){
        println("mee~~ mee~~");
    }
}


Mammal a = Cow();
Mammal b = Sheep();

//下面两个的叫声会不同。在运行期动态绑定方法。
a.speak();  
b.speak();


