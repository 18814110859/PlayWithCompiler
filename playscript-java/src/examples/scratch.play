class Mammal{
    void speak(){
        println("mammal speeking...");
    }
}

class Cow extends Mammal{
    Cow(){
        println("Cow constructor");
        speak();
    }
    void speak(){
        println("moo~~ moo~~");
    }
}

class Sheep extends Mammal{
    void speak(){
        println("mee~~ mee~~");
    }
}


Mammal a = Cow();
Mammal b = Sheep();

a.speak();
b.speak();


